<p id="notice"><%= notice %></p>

<h1>Listing Raitings</h1>

<table>
  <thead>
    <tr>
      <th>Critera</th>
      <th>Text</th>
      <th>Position</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @raitings.each do |raiting| %>
      <tr>
        <td><%= raiting.critera %></td>
        <td><%= raiting.text %></td>
        <td><%= raiting.position %></td>
        <td><%= link_to 'Show', raiting %></td>
        <td><%= link_to 'Edit', edit_raiting_path(raiting) %></td>
        <td><%= link_to 'Destroy', raiting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Raiting', new_raiting_path %>
